<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PrimeCut" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/vividBlue</item>
        <item name="colorPrimaryVariant">@color/midnightBlue</item>
        <item name="colorOnPrimary">@color/offWhite</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/slateGray</item>
        <item name="colorSecondaryVariant">@color/darkCharcoal</item>
        <item name="colorOnSecondary">@color/offWhite</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/midnightBlue</item>
    </style>

    <style name="ThemeOverlay.PrimeCut.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_900</item>
        <item name="fullscreenTextColor">@color/light_blue_A400</item>
    </style>
</resources>